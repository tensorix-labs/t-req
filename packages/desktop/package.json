{
  "name": "@t-req/desktop",
  "version": "0.1.1",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "sidecar:build": "bun run ./script/prepare-sidecar.ts",
    "rust:check": "sh -c 'TRIPLE=$(rustc --print host-tuple); SIDECAR=\"src-tauri/sidecars/treq-$TRIPLE\"; if [ ! -f \"$SIDECAR\" ]; then bun run sidecar:build; fi; cargo check --manifest-path src-tauri/Cargo.toml'",
    "tauri": "tauri",
    "tauri:dev": "bun run sidecar:build && tauri dev",
    "tauri:build": "bun run sidecar:build && tauri build"
  },
  "license": "MIT",
  "dependencies": {
    "@t-req/sdk": "workspace:*",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "solid-js": "^1.9.3"
  },
  "devDependencies": {
    "@t-req/ui": "workspace:*",
    "@tailwindcss/vite": "^4.1.0",
    "tailwindcss": "^4.1.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-plugin-solid": "^2.11.0",
    "@tauri-apps/cli": "^2"
  }
}
